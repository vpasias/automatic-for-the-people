network:
  version: 2
  renderer: networkd
  ethernets:
    eth1:
      dhcp4: false
      addresses: [ {{ server_ip }} ]
      routes:
        - to: 172.16.0.0/16
          via: {{ gateway }}
    vlans:
        eth1.100:
            id: 100
            link: eth1
            dhcp4: no
            optional: true
        eth1.200:
            id: 200
            link: eth1
            dhcp4: no
            optional: true
